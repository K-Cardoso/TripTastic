<p id="title">New Destination Page!</p>
<%= form_for :destination, url: destinations_path do |f| %>
  <p>
    <%= f.label :country %><br>
    <%= f.text_field :country %>
  </p>
 
  <p>
    <%= f.label :city %><br>
    <%= f.text_field :city %>
  </p>
  
  <p>
    <%= f.label :start_date %><br>
    <%= f.date_field :start_date %>
  </p>
  
  <p>
    <%= f.label :end_date %><br>
    <%= f.date_field :end_date %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
   <li><%= link_to 'Back', root_path %></li>
<% end %>